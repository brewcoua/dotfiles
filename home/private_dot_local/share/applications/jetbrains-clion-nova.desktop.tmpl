[Desktop Entry]
Version=1.4
Type=Application
Name=CLion Nova
Comment=A cross-platform IDE for C and C++
Categories=Development;IDE;
Icon={{ env "HOME" }}/.local/share/{{ .jetbrains.folder }}/icons/clion

Exec={{ trimSuffix "\n" (output "systemd-path" "user-binaries") }}/clion %f

Terminal=false
StartupWMClass=jetbrains-clion
StartupNotify=true
